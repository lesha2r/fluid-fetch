{
  "name": "fluid-fetch",
  "version": "0.0.2",
  "description": "Fetching package tiny enough for client apps. Zero dependencies. Fluid (chainable) API.",
  "keywords": [
    "fetch",
    "axios",
    "api",
    "data-fetching",
    "node-fetch",
    "api-client"
  ],
  "license": "ISC",
  "author": "lesha2r",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc -b src/",
    "dev": "nodemon dist/__dev__/index.js",
    "tsc": "tsc -b src/",
    "tscw": "tsc --b --watch src/",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "typescript": "^5.8.3",
    "@types/node": "^22.15.18"
  },
  "dependencies": {}
}
